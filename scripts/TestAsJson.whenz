#!/usr/bin/env whenz

when event app_starts do
	@buf is 1024 bytes
	@theFile = ./scripts/test-json.json
	read @theFile into @buf
	
when @buf.monitor is eof do once
	print The file has been read
	@json as json from @buf
	trigger event print
	
when event print do
	print @json.root.properties.name
	print Test passed
	exit
	